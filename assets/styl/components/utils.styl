@import "../mixins";

.clearfix
    *zoom 1

    &:before, &:after
        content ""
        display table
        line-height 0        

    &:after
        clear both

.unstyled
    unstyled()

.fixed-content
    margin-left auto
    margin-right auto
    max-width 980px
    padding-left 10px
    padding-right 10px
